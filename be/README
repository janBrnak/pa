## Docker Engine

- docker build -t pa-be .
- docker commit 

- docker run --rm -it -v "$PWD:/app" node bash
- docker run -d --net=pa --name pa node
- docker run --detach --network=host --name=pa-be --volume=$PWD:/usr/src/app --publish=3333:3333 pa-be
- docker run --interactive --tty --volume=$PWD:/usr/src/app --publish=3333:3333 --publish=8080:8080 --name=pa-be --env OG_HOST=192.168.0.55 --env OG_PORT=8090 node:latest ./bin/bash

- docker exec -it <CONTAINER_NAME> bash
- docker network ls

## Docker Compose

## Docker Machine
- docker-machine ls
- docker-machine ip <MACHINE_NAME>